@inherits ComponentBase;

<MudAppBar Dense="true" Elevation="1">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Edge="Edge.Start" />
    <MudText Typo="Typo.subtitle1" Class="ml-3">Curriculo Inteligente</MudText>
    <MudSpacer />

    <MudMenu Dense="true">
        <ActivatorContent>            
            <MudChip Icon="@Icons.Material.Filled.Person" Color="Color.Primary">Conta</MudChip>
        </ActivatorContent>
        <ChildContent>
            <MudMenuItem Icon="@Icons.Filled.PersonPin" Href="/conta" Disabled="true">Editar Conta</MudMenuItem>
        </ChildContent>        
    </MudMenu>    
</MudAppBar>
<MudDrawer Open="true" ClipMode="DrawerClipMode.Always" Elevation="2" Variant="@DrawerVariant.Mini" PreserveOpenState="false">
    <MudNavMenu Class="mud-width-full">
        <MudNavLink Href="/conta" Icon="@Icons.Filled.Security" Disabled="false">Conta</MudNavLink>
    </MudNavMenu>
</MudDrawer>


